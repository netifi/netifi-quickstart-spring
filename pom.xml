    <repositories>
        <repository>
            <id>netifi-oss</id>
            <url>https://dl.bintray.com/netifi/netifi-oss/</url>
        </repository>
        <repository>
            <id>jcenter</id>
            <url>https://jcenter.bintray.com/</url>
        </repository>
